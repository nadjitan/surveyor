{
  "name": "@nadjitan/surveyor",
  "version": "1.1.26",
  "private": false,
  "description": "A library for a capstone project",
  "main": "dist/surveyor.umd.js",
  "module": "dist/surveyor.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/surveyor.cjs.js",
      "import": "./dist/surveyor.es.js"
    },
