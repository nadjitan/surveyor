  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nadjitan/surveyor"
  },
  "keywords": [
    "react",
    "vite",
    "library",
    "typescript",
    "package"
  ],
  "author": "nadjitan",
  "homepage": "https://github.com/nadjitan/surveyor#readme",
  "scripts": {
