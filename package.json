    "react-dom": "^18.0.0",
    "set-distance": "^1.0.1",
    "tailwindcss": "^3.1.5",
    "toastify-js": "^1.12.0",
    "tsc-alias": "^1.6.10",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
